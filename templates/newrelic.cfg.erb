pidfile=<%= @daemon_pidfile %>
logfile=<%= @daemon_logfile %>
loglevel=<%= @daemon_loglevel %>
port="<%= @daemon_port -%>"
ssl=<%= @daemon_ssl %>
<% if @daemon_proxy -%>
proxy="<%= @daemon_proxy -%>"
<% end -%>
max_threads=<%= @daemon_max_threads %>
collector_host="<%= @daemon_collector_host -%>"
<% if @daemon_auditlog -%>
auditlog="<%= @daemon_auditlog -%>"
<% end -%>
